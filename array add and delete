// Online C compiler to run C program online
#include <stdio.h>
void insert(int a[1000],int n){
     printf("enter elements");
     for(int i =0;i<n;i++){
         scanf("%d",&a[i]);
     }
}
     void display(int a[100],int n){
         for(int i =0;i<n;i++){
         printf("%d",a[i]);
     }
     }
     int main(){
         while(1){
         int n,a[1000],ch;
         printf("enter 1 for insertion 2 for display");
         scanf("%d",&ch);
         switch(ch){
             case 1:{
             printf("enter no of terms");
             scanf("%d",&n);
             insert(a,n);
             break;
             
         }
         case 2:
         display(a,n);
     }
}
}

